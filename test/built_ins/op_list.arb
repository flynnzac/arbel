#!./arbel -c
source "src/library/assert.arb" .
error-messages False .

print "Start <list> test." .
' Tests with correct usage

set /x [ list 1 2 3 . ] .
assert /test ( eq x:/#1 1 . ) .
assert /test ( eq x:/#2 2 . ) .
assert /test ( eq x:/#3 3 . ) .
assert /test ( register-eq [ last x "#" . ] /#3 . ) .

set /y [ list . ] .
assert /test ( not [ exist /#0 y . ] . ) .

