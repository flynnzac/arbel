' takes a function as an argument and return left hand
' riemmann sum over range

set :integrate 
    ( 
      mov :#1 :f .
      mov :#2 :lb .
      mov :#3 :ub .
      mov :#4 :n .
      set :delta [ divide [ subtract \ub \lb . ] \n . ] .
      set :x \lb .
      set :int 0.0 .
      while ( lt \x \ub . )
            (
              set :int [ add \int [ multiply [ f \x . ] \delta . ] . ] .
              set :x [ add \x \delta . ] .
            ) .
      answer int .
    ) .
