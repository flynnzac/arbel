c parses a comma-separated file into a registry

source "examples/tokenize.arb" .

set $parse-csv
    (
      open-text-file \#1 .
      set $file ans .
      set $parsed [ list . ] .
      set $cur-reg $#1 . 

      while ( set $cur-line [ read-line file . ] . is-string cur-line . )
            (
              tokenize cur-line "," .
              in \parsed ( set cur-reg ans . ) .
              set $cur-reg [ next cur-reg . ] .
            ) .
      
      close $file .
      answer \parsed .
    ) .
